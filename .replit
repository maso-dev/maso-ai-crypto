run = "python -m uvicorn main:app --host 0.0.0.0 --port 8000"
entrypoint = "main.py"

[nix]
channel = "stable-22_11"

[env]
PYTHONPATH = "${REPL_HOME}"

[packager]
language = "python3"

[packager.features]
packageSearch = true
guessImports = true

[languages]
[languages.python]
pattern = "**/*.py"
syntax = "python"

[languages.javascript]
pattern = "**/*.js"
syntax = "javascript"

[languages.html]
pattern = "**/*.html"
syntax = "html"

[languages.css]
pattern = "**/*.css"
syntax = "css"
