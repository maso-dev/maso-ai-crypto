{
    "version": 2,
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/static/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "crons": [
        {
            "path": "/populate_crypto_news_rag",
            "schedule": "0 6 * * *"
        },
        {
            "path": "/admin/health",
            "schedule": "0 */6 * * *"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 30,
            "runtime": "python3.9"
        }
    }
}
