{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/static/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "crons": [
        {
            "path": "/populate_crypto_news_rag",
            "schedule": "0 6 * * *"
        },
        {
            "path": "/admin/health",
            "schedule": "0 */6 * * *"
        }
    ]
}
