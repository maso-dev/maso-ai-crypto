{
  "version": 2,
  "builds": [
    {
      "src": "main-vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main-vercel.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "main-vercel.py": {
      "maxDuration": 60
    }
  },
  "regions": [
    "iad1"
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.10"
    }
  }
}
